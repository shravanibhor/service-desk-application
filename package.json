{
  "name": "service-desk-app",
  "version": "1.0.0",
  "description": "Full-stack Service Desk Application with React frontend and Node.js backend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
